{
  "name": "classroom-desktop",
  "productName": "GitHub Classroom",
  "version": "0.4.0",
  "description": "Desktop helper for GitHub Classroom",
  "main": "app/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js .",
    "test-unit": "cross-env TEST_ENV=1 ELECTRON_NO_ATTACH_CONSOLE=1 xvfb-maybe --auto-servernum -- electron-mocha -t 15000 --require babel-core/register --require ./test/setup app/**/*-test.js",
    "test": "npm run lint && npm run test-unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/education/classroom-desktop/"
  },
  "keywords": [
    "GitHub",
    "Classroom",
    "Helper",
    "Education",
    "School",
    "Repository"
  ],
  "author": "GitHub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/education/classroom-desktop/issues"
  },
  "homepage": "https://github.com/education/classroom-desktop#readme",
  "engines": {
    "node": ">= 8"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "classroom-desktop"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "classroom-desktop"
      }
    }
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "bootstrap": "~4.1.1",
    "classnames": "^2.2.6",
    "dateformat": "^3.0.3",
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "^2.1.0",
    "electron-is-dev": "~0.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "font-awesome": "~4.7.0",
    "http-link-header": "^0.8.0",
    "nodegit": "^0.21.2",
    "opener": "^1.4.1",
    "prop-types": "^15.6.1",
    "react": "~16.2.0",
    "react-dom": "~16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "~5.0.7",
    "react-router": "~3.0.2",
    "react-router-redux": "~4.0.5",
    "react-spinners": "^0.2.6",
    "redux": "~3.7.2",
    "redux-devtools": "~3.4.1",
    "redux-devtools-dock-monitor": "~1.1.1",
    "redux-devtools-log-monitor": "~1.4.0",
    "redux-mock-store": "^1.1.2",
    "redux-thunk": "~2.2.0",
    "reselect": "~3.0.1",
    "underscore": "^1.8.3",
    "winston": "~2.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.4",
    "electron": "~1.8.4",
    "electron-forge": "^5.1.1",
    "electron-mocha": "^6.0.1",
    "electron-packager": "~11.2.0",
    "electron-prebuilt-compile": "1.8.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "~11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "~3.7.0",
    "eslint-plugin-react": "~7.7.0",
    "eslint-plugin-standard": "~3.0.1",
    "mocha": "~5.0.5",
    "rimraf": "~2.6.2",
    "sinon": "^4.5.0",
    "xvfb-maybe": "^0.2.1"
  }
}
